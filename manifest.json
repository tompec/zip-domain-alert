{
    "manifest_version": 3,
    "name": ".zip Domain Alert",
    "version": "0.0.2",
    "description": "An extension to change every link with .zip TLD to have a red border",
    "icons": {
        "16": "images/16.png",
        "32": "images/32.png",
        "48": "images/48.png",
        "128": "images/128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "script.js"
            ]
        }
    ]
}
